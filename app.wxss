/* 顶部搜索 */

.userinfo_Msg {
  height: 100%;
  display: flex;
  flex-direction: column;
}

.search_Box {
  padding: 24rpx 18rpx 0rpx 19rpx;
  height: 87rpx;
}

.search_Msg {
  padding: 10rpx 16rpx 10rpx 19rpx;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
  z-index: 10；
}

.search_Icon {
  display: flex;
  align-items: center;
  flex: 1;
}
.search_Input {
  flex: 1;
}

.search_close {
  display: flex;
  align-items: center;
}

.search_close i {
  font-size: 24rpx;
  color: #343434;
  padding-left: 40rpx;
}

.search_Box {
  position: relative;
  z-index: 10;
}

.search_selected {
  position: absolute;
  width: 96%;
  background: #DEDFE5;
  padding: 0 50rpx;
  margin-top: 10rpx;
  border-radius: 10rpx;
  z-index: 10;
}
.search_selected_Item{
padding: 20rpx;
border-bottom: 1rpx solid #EEEEEE;
}

.search_selected_Item:last-child {
  border: none;
}

.mask-popup{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  z-index: 2;
}

